<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jky.verify.modules.task.mapper.TaskInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jky.verify.modules.task.bean.TaskInfo">
        <id column="id" property="id" />
        <result column="task_name" property="taskName" />
        <result column="task_key" property="taskKey" />
        <result column="cron" property="cron" />
        <result column="param" property="param" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, task_name, task_key, cron, param
    </sql>

</mapper>
